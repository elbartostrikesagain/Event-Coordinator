-unless devise_error_messages!.blank?
  .alert.alert-error= devise_error_messages!

.well
  = form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
    %fieldset
      %legend Sign up
    .control-group
      = f.label :email
      = f.text_field :email
    .control-group
      = f.label :name
      = f.text_field :name
    -unless session["omniauth"]
      .control-group
        = f.label :password
        = f.password_field :password
      .control-group
        = f.label :password_confirmation
        = f.password_field :password_confirmation
    %p= f.submit "Sign up", class: 'btn btn-primary'
  - if !user_signed_in? && !session[:omniauth]
    = render :partial => 'authentications/sign_up', :locals => {:source => "sign_up"}
  = render :partial => "devise/shared/links"